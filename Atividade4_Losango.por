programa {
  funcao inicio() {

   real diagonalMaior
   real diagonalMenor

   escreva("escreva a diagonal maior: ")
   leia(diagonalMaior)

   escreva("escreva a diagonal menor: ")
   leia(diagonalMenor)

   real resultado = diagonalMaior * diagonalMenor / 2

